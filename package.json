{
  "name": "sdl2",
  "version": "2.0.5",
  "description": "PackageJson version of SDL2.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "esy": {
    "build": ["./configure --prefix=$cur__lib --libdir=$cur__lib && make && make install"],
    "exportedEnvVars": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "$sdl2__lib/pkgconfig:$PKG_CONFIG_PATH"
      }
    },
    "buildsInSource": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reasonml/SDL-mirror.git"
  },
  "keywords": [
    "sdl",
    "sdl2"
  ],
  "author": "",
  "license": "Zlib",
  "bugs": {
    "url": "https://github.com/bsansouci/SDL-mirror/issues"
  },
  "homepage": "https://github.com/bsansouci/SDL-mirror#readme"
}
